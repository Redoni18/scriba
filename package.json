{
  "name": "scriba-news-aggregator",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch index.ts",
    "build": "bun build ./index.ts --outdir ./out --target node",
    "start": "bun run ./out/index.js",
