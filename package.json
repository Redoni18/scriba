    "prisma:push": "bunx prisma db push"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prisma": "^5.4.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2"
  }
}

