{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "crons": [
      {
        "path": "/api/fetchNews",
        "schedule": "0 0 * * *"
      }
    ]
  }
  
  